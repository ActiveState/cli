{
  "env": [
    {
      "env_name": "PATH",
      "join": "prepend",
      "inherit": true,
      "values": [
        "${INSTALLDIR}/bin"
      ],
      "separator": ";"
    }
  ],
  "file_transforms": [
    {
      "pattern": "PLACEHOLDER",
      "with": "ANOTHER",
      "in": [
        "bin/test-another-offline-install"
      ]
    }
  ],
  "installdir": "installdir"
}